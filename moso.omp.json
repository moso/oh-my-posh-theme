{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#f38ba8",
                    "style": "plain",
                    "template": "# {{ .UserName | lower }} ",
                    "type": "session"
                },
                {
                    "foreground": "#cdd6f4",
                    "style": "plain",
                    "template": "@",
                    "type": "text"
                },
                {
                    "foreground": "#74c7ec",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "plain",
                    "template": " {{ .Path }}",
                    "type": "path"
                },
                {
                    "foreground": "#b4befe",
                    "properties": {
                        "branch_icon": " \ufb2b"
                    },
                    "style": "plain",
                    "template": " <#a6e3a1>\uf408</> {{ .HEAD }}<#a6e3a1></>",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#f9e2af",
                    "style": "plain",
                    "template": "\ufb0c",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
