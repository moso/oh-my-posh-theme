{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#FF80BF",
                    "style": "plain",
                    "template": "#{{ .UserName }} ",
                    "type": "session"
                },
                {
                    "foreground": "#FFFFFF",
                    "style": "plain",
                    "template": "@",
                    "type": "text"
                },
                {
                    "foreground": "#86DDF0",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "plain",
                    "template": " {{ .Path }}",
                    "type": "path"
                },
                {
                    "foreground": "#9384FF",
                    "properties": {
                        "branch_icon": "\ue0a0 "
                    },
                    "style": "plain",
                    "template": " <#8AFF80>git(</>{{ .HEAD }}<#8AFF80>)</>",
                    "type": "git"
                },
                {
                    "foreground": "#FFFFFF",
                    "style": "plain",
                    "template": " ",
                    "type": "exit"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#63F08C",
                    "style": "plain",
                    "template": "➜ ",
                    "type": "text"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}
